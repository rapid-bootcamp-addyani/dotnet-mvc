@model LoanInventoryViewModel

@{
    ViewData["Title"] = "Product Details";
}

<h2>Product Detail</h2>
<table class="table tab-content">
    <tbody>
        <tr>
            <th>ID</th>
            <td>@Model.Id</td>
        </tr>
        <tr>
            <th>User Name</th>
            <td>@Model.NameUser</td>
        </tr>
        <tr>
            <th>Detail</th>
            <td>@Model.Detail</td>
        </tr>
        <tr>
            <th>Detail</th>
            <td>@Model.Admin</td>
        </tr>
        <tr>
            <th>Detail</th>
            <td>@Model.Status</td>
        </tr>
        <tr>
            <th>Date</th>
            <td>@Model.Date</td>
        </tr>

    </tbody>
</table>

<a class="btn btn-sm btn-primary" asp-area="" asp-controller="LoanInventory" asp-action="List">Back</a>
