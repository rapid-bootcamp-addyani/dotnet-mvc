@model EmployeeViewModel

@{
    ViewData["Title"] = "Employee Details";
}

<h2>Employee Detail</h2>
<table class="table tab-content">
    <tbody>
        <tr>
            <th>ID</th>
            <td>@Model.Id</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>@Model.EmployeeName</td>
        </tr>
        <tr>
            <th>Email</th>
            <td>@Model.Email</td>
        </tr>
    
        <tr>
            <th>Phone</th>
            <td>@Model.Phone</td>
        </tr>

        <tr>
            <th>Address</th>
            <td>@Model.Address</td>
        </tr>

        <tr>
            <th>Position</th>
            <td>@Model.Position</td>
        </tr>

    </tbody>
</table>

<a class="btn btn-sm btn-primary" asp-area="" asp-controller="Employee" asp-action="List">Back</a>
